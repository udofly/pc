{
    "name": "PC",
    "version": "1.0.0",
    "private": true,
    "description": "Pc",
    "scripts": {
        "start:test": "vite --mode=test",
        "start:dev": "vite --mode=dev",
        "start:dev-hwm": "vite --mode=dev-hwm",
        "start:prod": "vite --mode=prod",
        "start:prod-hwm": "vite --mode=prod-hwm",
        "build:test": "vite build --mode=test",
        "build:prod": "vite build --mode=prod",
        "build:prod-hwm": "vite build --mode=prod-hwm",
        "build:dev": "vite build --mode=dev",
        "build:dev-hwm": "vite build --mode=dev-hwm",
        "build-tsc": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "format": "prettier --write ./src",
        "lint": "eslint ./src --ext .vue,.js,.ts",
        "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
        "prepare": "husky install config/.husky"
    },
    "keywords": [
        "Vite",
        "Vue3",
        "TypeScript",
        "Starter"
    ],
    "web-types": "src/utils/types/web-types.json",
    "author": "~~",
    "browserslist": [
        "> 0.5%",
        "last 8 versions",
        "android >4.4",
        "chrome >69"
    ],
    "license": "MIT",
    "lint-staged": {
        "*.{vue,js,ts}": "eslint --fix"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ],
        "rules": {
            "type-enum": [
                2,
                "always",
                [
                    "bug",
                    "feat",
                    "fix",
                    "docs",
                    "style",
                    "refactor",
                    "test",
                    "chore",
                    "revert",
                    "merge"
                ]
            ]
        }
    },
    "dependencies": {
        "@ant-design/icons-vue": "^6.1.0",
        "@popjs/core": "^0.5.2",
        "@sentry/tracing": "^6.19.7",
        "@sentry/vue": "^6.19.7",
        "@vant/touch-emulator": "^1.3.2",
        "@vant/use": "^1.3.6",
        "@vuelidate/core": "^2.0.0-alpha.41",
        "@vuelidate/validators": "^2.0.0-alpha.29",
        "ali-oss": "^6.17.1",
        "ant-design-vue": "^2.2.8",
        "axios": "^0.27.2",
        "crypto-js": "^4.1.1",
        "dayjs": "^1.11.1",
        "dotenv": "^16.0.0",
        "element-plus": "^2.1.11",
        "lodash-es": "^4.17.21",
        "mitt": "^3.0.0",
        "push.js": "^1.0.12",
        "socket.io-client": "^2.4.0",
        "ua-parser-js": "^1.0.2",
        "vite-plugin-mkcert": "^1.6.0",
        "vue": "3.2.33",
        "vue-router": "^4.0.14",
        "vue3-clipboard": "^1.0.0",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@types/socket.io-client": "^1.4.36",
        "@commitlint/cli": "^16.2.4",
        "@commitlint/config-conventional": "^16.2.4",
        "@types/crypto-js": "^4.1.1",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^17.0.31",
        "@types/ua-parser-js": "^0.7.36",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "@vitejs/plugin-vue": "^2.3.1",
        "@vue/compiler-sfc": "^3.2.33",
        "browserslist": "^4.20.3",
        "eslint": "^8.14.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-vue": "^8.7.1",
        "husky": "^7.0.4",
        "less": "^4.1.2",
        "typescript": "^4.6.4",
        "vite": "^2.9.6",
        "vite-plugin-sentry": "^1.0.14",
        "vue-tsc": "^0.34.11"
    }
}
